{
	"name": "@rassweiler/kawasaki-as-parser",
	"version": "2.0.0",
	"description": "Kawasaki backup parser",
	"main": "lib/index.js",
	"scripts": {
		"mocha": "mocha --require @babel/register",
		"build": "babel src/index.ts --out-dir lib --extensions \".ts\"",
		"test": "yarn build && yarn mocha",
		"preversion": "yarn test",
		"version": "git add -A",
		"postversion": "git push && git push --tags",
		"lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
		"lint:fix": "eslint . --fix --cache --ext .js,.jsx,.ts,.tsx"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rassweiler/kawasaki-as-parser.git"
	},
	"keywords": [
		"Kawasaki",
		"robot",
		"parser",
		"backup"
	],
	"author": "Kyle Rassweiler",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/rassweiler/kawasaki-as-parser/issues"
	},
	"homepage": "https://github.com/rassweiler/kawasaki-as-parser#readme",
	"files": [
		"LICENSE",
		"README.md",
		"index.js",
		"lib/"
	],
	"devDependencies": {
		"@babel/cli": "^7.6.0",
		"@babel/core": "^7.5.5",
		"@babel/node": "^7.5.5",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/plugin-transform-runtime": "^7.6.0",
		"@babel/preset-env": "^7.5.5",
		"@babel/preset-typescript": "^7.12.7",
		"@babel/register": "^7.5.5",
		"@babel/runtime": "^7.6.0",
		"@typescript-eslint/eslint-plugin": "^4.21.0",
		"@typescript-eslint/parser": "^4.21.0",
		"chai": "*",
		"eslint": "^7.24.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-prettier": "^3.3.1",
		"mocha": "*",
		"nyc": "^15.1.0",
		"prettier": "^2.2.1",
		"typescript": "^4.2.4"
	}
}
